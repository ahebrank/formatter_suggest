<?php


/**
 * @file
 * Primary module hooks for Formatter Suggest test module.
 */

/**
 * Implments hook_theme().
 */
function formatter_suggest_test_theme($existing, $type, $theme, $path) {
  return [
    'field__node__page__suggested' => [
      'template' => 'field--node--page--suggested',
      'path' => drupal_get_path('module', 'formatter_suggest_test') . '/templates',
    ],
  ];
}